<h1 class="text-center m-4">All Articles</h1>

<div class="flickr_pagination d-flex justify-content-center  m-4">
    <%= will_paginate @articles %>
  </div>


<div class="container d-flex flex-wrap ">
    <% @articles.each do |f| %>
    <%= render "layouts/card_article", article: f %>    
  <% end %>
</div>

